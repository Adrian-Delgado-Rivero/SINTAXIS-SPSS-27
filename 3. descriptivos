*############ ##############################  FRECUENCIAS ############################################################
*CON CUARTILES (NTILES   =4)
*PUNTOS DE CORTE PARA 10 GRUPOS IGUALES (NTILES =10)
Â¨*CON PERCENTILES 

DATASET ACTIVATE ConjuntoDatos2.
FREQUENCIES VARIABLES=depto area tothrs
  /FORMAT=NOTABLE
  /NTILES=4
  /NTILES=10
  /PERCENTILES=5.0 35.0 50.0 75.0 95.0 
  /STATISTICS=STDDEV VARIANCE RANGE MINIMUM MAXIMUM SEMEAN MEAN MEDIAN MODE SUM SKEWNESS SESKEW 
    KURTOSIS SEKURT
  /ORDER=ANALYSIS.


*####################     ##############################     DESCRIPTIVOS ############################################################

*sin guardar una nueva variable estadarizada

DESCRIPTIVES VARIABLES=tothrs 
  /STATISTICS=MEAN SUM STDDEV VARIANCE RANGE MIN MAX SEMEAN KURTOSIS SKEWNESS
  /SORT=NAME (A).

*guardando una  nueva variable estandarizada

DESCRIPTIVES VARIABLES=tothrs 
  /SAVE
  /STATISTICS=MEAN SUM STDDEV VARIANCE RANGE MIN MAX SEMEAN KURTOSIS SKEWNESS
  /SORT=NAME (A).

*usando la variable original y la estadarizada

DESCRIPTIVES VARIABLES=tothrs Ztothrs
  /STATISTICS=MEAN SUM STDDEV VARIANCE RANGE MIN MAX SEMEAN KURTOSIS SKEWNESS
  /SORT=NAME (A).

*###################################EXPLORAR############################################################

*con estadisticos y sin graficos ( /PLOT NONE)
#descriptivos al 95%
 ( /STATISTICS DESCRIPTIVES, /CINTERVAL 95)

                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT NONE
                                      /STATISTICS DESCRIPTIVES
                                      /CINTERVAL 95
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#Estimadores M
                                       (  /MESTIMATORS HUBER(1.339) ANDREW(1.34) HAMPEL(1.7,3.4,8.5) TUKEY(4.685)
                                      /STATISTICS NONE
                                      /MISSING LISTWISE)
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT NONE
                                      /MESTIMATORS HUBER(1.339) ANDREW(1.34) HAMPEL(1.7,3.4,8.5) TUKEY(4.685)
                                      /STATISTICS NONE
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#valores atipicos (  /STATISTICS EXTREME)
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT NONE
                                      /STATISTICS EXTREME
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#precentiles
                                     (  /PERCENTILES(5,10,25,50,75,90,95) HAVERAGE
                                      /STATISTICS NONE)
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT NONE
                                      /PERCENTILES(5,10,25,50,75,90,95) HAVERAGE
                                      /STATISTICS NONE
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    

*todos los estadisticos


EXAMINE VARIABLES=ylab BY tothrs
  /ID=area
  /PLOT NONE
  /MESTIMATORS HUBER(1.339) ANDREW(1.34) HAMPEL(1.7,3.4,8.5) TUKEY(4.685)
  /PERCENTILES(5,10,25,50,75,90,95) HAVERAGE
  /STATISTICS DESCRIPTIVES EXTREME
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.


                                    
                                    *sin estadisticos y con graficos (   /STATISTICS NONE)
                                    *#diagramas de cajas: "niveles de los factores juntos" 
                                    ( /PLOT BOXPLOT
                                      /COMPARE GROUPS)
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT BOXPLOT
                                      /COMPARE GROUPS
                                      /STATISTICS NONE
                                      /CINTERVAL 95
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#diagramas de cajas: "dependientes juntos"
                                    ( /PLOT BOXPLOT
                                      /COMPARE VARIABLES)
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT BOXPLOT
                                      /COMPARE VARIABLES
                                      /STATISTICS NONE
                                      /CINTERVAL 95
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#diagramas de cajas: ninguno"
                                    descriptivo: tallo y hoja  (  /PLOT STEMLEAF)
                                    
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT STEMLEAF
                                      /STATISTICS NONE
                                      /CINTERVAL 95
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#diagramas de cajas: ninguno"
                                    descriptivo: histograma  (    /PLOT HISTOGRAM)
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT HISTOGRAM
                                      /STATISTICS NONE
                                      /CINTERVAL 95
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#diagramas de cajas: ninguno"
                                    descriptivo: ninguno
                                    grafico de normalidad con pruebas (  /PLOT NPPLOT)
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT NPPLOT
                                      /STATISTICS NONE
                                      /CINTERVAL 95
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#diagramas de cajas: ninguno"
                                    descriptivo: ninguno
                                    grafico:  dispercion vs nivel con prueba de levene
                                     (  /PLOT SPREADLEVEL)
                                    
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT SPREADLEVEL
                                      /STATISTICS NONE
                                      /CINTERVAL 95
                                      /MISSING LISTWISE
                                      /NOTOTAL.
                                    
                                    *#diagramas de cajas: ninguno"
                                    descriptivo: ninguno
                                    grafico:  dispercion vs nivel con prueba de levene 
                                    TRANSFORMADOS: (  /PLOT SPREADLEVEL(#)  )
                                                LOG NATURAL (0)
                                                1/RAIZ CUADRADA  (-0.5)
                                                RECIPROCO (-1)
                                                RAIZ CUADRADA (0.5)
                                                CUADRADO (2)
                                                CUBO (3)
                                                NO TRANSFORMADOS (1)
                                    
                                    EXAMINE VARIABLES=ylab BY tothrs
                                      /ID=area
                                      /PLOT SPREADLEVEL(0)
                                      /STATISTICS NONE
                                      /CINTERVAL 95
                                      /MISSING LISTWISE
                                      /NOTOTAL.

*#TODOS LOS GRAFICOS (dependientes junto y estimacion en potencias)

EXAMINE VARIABLES=ylab BY tothrs
  /ID=area
  /PLOT BOXPLOT STEMLEAF HISTOGRAM SPREADLEVEL
  /COMPARE VARIABLES
  /STATISTICS NONE
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.

*#estadisticos y graficos 

EXAMINE VARIABLES=ylab BY tothrs
  /ID=area
  /PLOT BOXPLOT STEMLEAF HISTOGRAM SPREADLEVEL
  /COMPARE VARIABLES
  /MESTIMATORS HUBER(1.339) ANDREW(1.34) HAMPEL(1.7,3.4,8.5) TUKEY(4.685)
  /PERCENTILES(5,10,25,50,75,90,95) HAVERAGE
  /STATISTICS DESCRIPTIVES EXTREME
  /CINTERVAL 95
  /MISSING LISTWISE
  /NOTOTAL.

